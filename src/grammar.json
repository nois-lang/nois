{
  "rules": [
    {
      "name": "program",
      "branches": [
        [
          "statement"
        ]
      ]
    },
    {
      "name": "statement",
      "branches": [
        [
          "function-def"
        ]
      ]
    },
    {
      "name": "function-def",
      "branches": [
        [
          "fn-keyword",
          "identifier",
          "params",
          "colon",
          "identifier",
          "block"
        ]
      ]
    },
    {
      "name": "params",
      "branches": [
        [
          "open-paren",
          "params_",
          "close-paren"
        ],
        [
          "open-paren",
          "close-paren"
        ]
      ]
    },
    {
      "name": "params_",
      "branches": [
        [
          "param",
          "comma",
          "params_"
        ],
        [
          "param",
          "trailing-comma"
        ],
        [
          "param"
        ]
      ]
    },
    {
      "name": "param",
      "branches": [
        [
          "identifier",
          "colon",
          "identifier"
        ]
      ]
    },
    {
      "name": "trailing-comma",
      "branches": [
        [
          "comma"
        ],
        [
          "e"
        ]
      ]
    },
    {
      "name": "block",
      "branches": [
        [
          "open-brace",
          "block_",
          "close-brace"
        ],
        [
          "open-brace",
          "close-brace"
        ]
      ]
    },
    {
      "name": "block_",
      "branches": [
        [
          "e"
        ]
      ]
    }
  ]
}
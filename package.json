{
  "name": "nois",
  "version": "0.0.1",
  "description": "Statically typed programming language for the web",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc && npm run build:std",
    "build:lib": "tsc -p tsconfig.lib.json && npm run build:std",
    "test": "jest --watchAll",
    "ci": "jest --ci",
    "build:std": "cp -r src/std dist/",
    "postinstall": "npm run build:lib",
    "pkg": "pkg -t node18-linux ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nois-lang/nois.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nois-lang/nois/issues"
  },
  "bin": {
    "nois": "dist/index.js"
  },
  "pkg": {
    "assets": [
      "dist/std/**/*"
    ]
  },
  "homepage": "https://github.com/nois-lang/nois#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/node": "~18.16.3",
    "@types/jasmine": "~4.3.1",
    "typescript": "~5.0.4",
    "ts-node": "~10.9.1",
    "jest": "~29.5.0",
    "@jest/globals": "~29.5.0",
    "@swc/core": "~1.3.56",
    "@swc/jest": "~0.2.26",
    "pkg": "~5.8.1"
  }
}
